{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "es2015.promise", "dom", "es2015"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
